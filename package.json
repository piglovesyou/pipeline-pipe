{
  "name": "@piglovesyou/parallel-transform",
  "version": "2.1.2",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "git@github.com:piglovesyou/parallel-transform.git",
  "license": "MIT",
  "description": "Transform stream that allows you to run your transforms in parallel without changing the order",
  "keywords": [
    "transform",
    "stream",
    "parallel",
    "preserve",
    "order"
  ],
  "files": [
    "src"
  ],
  "author": "Soichi Takamura <thepiglovesyou@gmail.com>",
  "dependencies": {
    "cyclist": "^1.0.1",
    "readable-stream": "^3.4.0"
  },
  "devDependencies": {
    "@types/inherits": "^0.0.30",
    "@types/jest": "^24.0.19",
    "@types/node": "^12.11.1",
    "@types/readable-stream": "^2.3.5",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run fix-js",
      "git add --force"
    ]
  },
  "scripts": {
    "fix": "yarn run lint-js --fix",
    "lint": "eslint --ext .ts --ext .tsx --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
    "prepack": "tsc --declaration",
    "test": "tsc --noEmit && jest",
    "test-watch": "yarn run test --watch --notify",
    "test-cover": "yarn run test --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
